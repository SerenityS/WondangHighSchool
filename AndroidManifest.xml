            android:name="wondang.icehs.kr.whdghks913.wondanghighschool.SettingsActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="설정"
            android:parentActivityName="wondang.icehs.kr.whdghks913.wondanghighschool.MainActivity"
